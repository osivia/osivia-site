fichePersonne.roleRazMdp=role_admin

ficheEtablissement.roleAdministrateur=role_admin

gestionapplications.roleAdministrateur=role_admin
gestiongroupes.roleAdministrateur=role_admin
gestionorganisations.roleAdministrateur=role_admin
gestionpersonnes.roleAdministrateur=role_admin

