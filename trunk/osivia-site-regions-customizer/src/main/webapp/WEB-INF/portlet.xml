<?xml version="1.0" encoding="UTF-8" ?>

<portlet-app
    xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
    version="2.0">

    <portlet>
        <portlet-name>osivia-site-regions-customizerPortlet</portlet-name>
        <portlet-class>org.osivia.portal.site.customizer.RegionsCustomizerPortlet</portlet-class>
        <expiration-cache>180</expiration-cache>

        <supports>
            <mime-type>text/html</mime-type>
        </supports>
          
        <portlet-info>
            <title>Regions customizer</title>
        </portlet-info>
        
        <init-param>
            <name>osivia.site.customizer.regions.osivia-site-charte.context</name>
            <value>/osivia-site-charte</value>
        </init-param>
        <init-param>
            <name>osivia.site.customizer.regions.osivia-site-charte-community.context</name>
            <value>/osivia-site-charte-community</value>
        </init-param> 
    </portlet>

</portlet-app>
